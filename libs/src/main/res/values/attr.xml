        <attr name="outPadding" format="dimension"></attr>
        <attr name="dividerHeight" format="dimension"></attr>
        <attr name="actionPadding" format="dimension"></attr>
        <attr name="isImmersive" format="boolean"></attr>

        <attr name="leftText" format="string"></attr>
        <attr name="leftTextBackgroundResource" format="reference"></attr>
        <attr name="leftTextBackgroundColor" format="color"></attr>
